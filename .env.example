# Environment Variables for Voice Healthcare Agent

# Deepgram API Key for Speech-to-Text
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# ElevenLabs API Key for Text-to-Speech
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Gemini AI API Key for Natural Language Processing
GEMINIAI_API_KEY=your_gemini_ai_api_key_here

# Server Port
PORT=3000

# Optional: ElevenLabs Voice ID (default will be used if not specified)
ELEVENLABS_VOICE_ID=your_preferred_voice_id